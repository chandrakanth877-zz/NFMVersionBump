{
    "manifestVersion": 1,
    "id": "nfmversionbump",
    "name": "NFM Version bump using Project file",
    "version": "0.0.20",
    "publisher": "chandrakanth",
    "public": true, 
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
